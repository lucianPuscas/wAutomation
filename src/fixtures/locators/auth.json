{
  "signupform": {
    "logo": "[alt='wakelet']"
  },
  "signup": {
    "signUpWithEmailButton": "button:contains('Sign up with email')",
    "nameInput": "input#name",
    "emailInput": "input#email",
    "passwordInput": "input#password",
    "termsAndConditionsCheckbox": "input#checkbox",
    "createYourAccountButton": "button.btn--primary:contains('Create your account')",
    "getStartedButton": "[data-click-track='OnboardStepGo']",
    "usernameInput": "input[aria-labelledby='paper-input-label-2']",
    "nextButton": "[data-click-track='OnboardStepProfileNext']:contains('Next')",
    "sectorNext": "paper-button#sectorNextButton",
    "sectorOptions": "div#prompt-container li.option:contains('Social media')",
    "doneButton": "paper-button:contains('Done')",
    "ageDDInput": "input#age-dd",
    "ageMMInput": "input#age-mm",
    "ageYYYYInput": "input#age-yyyy",
    "selectOptions":"li.option:contains('Social media')",
    "tripleDotButton":"[icon='wakelet:triple-dot']",
    "accountSettings":"[data-testid='header-settings-link']",
    "deleteAccount":"paper-button:contains('Delete account')",
    "deleteInputPass":"[data-testid='settings-change-password-confirmation-input']",
    "deleteButton":"paper-button:contains('Delete')"


  },
  "login": {
    "emailAddressInput": "input#email",
    "passwordInput": "input#password",
    "loginButton": "form button.btn--primary.gt--s",
    "error": {
      "invalidEmail": "label.input--error[data-error-type='invalid-email']",
      "invalidUsernameOrPassword": "label.input--error[data-error-type='invalid-email-or-password']",
      "tooManyWrongAttempts": "label.input--error[data-error-type='too-many-wrong-attempts']"
    }
  }
}
